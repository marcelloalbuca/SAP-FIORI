        <content>
            <ObjectHeader 
            title="Campeonato Brasileiro FioriNET"
            number="22ª"
            numberUnit="rodada"
            > 

            </ObjectHeader>

                 <IconTabBar>
                    <items>
                        <IconTabFilter text="Classificação" icon="sap-icon://competitor">
                        
                        </IconTabFilter>
                    </items>
                </IconTabBar>

                
                 <IconTabBar>
                    <items>
                        <IconTabFilter text="Partidas" icon="sap-icon://soccer">
                        
                        </IconTabFilter>
                    </items>
                </IconTabBar>

         </content>
    </Page>
</mvc:View>
