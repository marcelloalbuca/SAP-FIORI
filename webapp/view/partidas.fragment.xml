<c:FragmentDefinition
    xmlns="sap.m"
    xmlns:f="sap.f"
    xmlns:l="sap.ui.layout"
    xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:c="sap.ui.core"
>


    <Table id="partidasTable" headerText="Partidas"
        items="{ModeloPartidas>/partidas}"
        inset="false">
        <columns>
            <Column width="10%" hAlign="Right">
                <Text text="" />
            </Column>

            <Column width="10%" hAlign="Center">
                <Text text="Resultados" />
            </Column>

            <Column width="10%" hAlign="Left">
                <Text text="" />
            </Column>

        </columns>
        <items>
            <ColumnListItem vAlign="Middle">
                <cells>

                    <Image src="{ModeloPartidas>time_mandante/escudo}" width="15%" />
                        <VBox>

                            <Text text="{ModeloPartidas>placar}" />
                            <Text text="02/10 - 19h" />
                            <Text text="Encerrada" />

                        </VBox>
                    <Image src="https://1.bp.blogspot.com/-NzAUmbZFcgo/XgXrFsyj-QI/AAAAAAAAH1E/qiHs2Zh_Y_sbnwZUFmDMvk1JtV1_sAvAACLcBGAsYHQ/s1600/ESPORTE%2BCLUBE%2BBAHIA%2B%25282%2529.png" width="15%" />
                
                 </cells>
            </ColumnListItem>

        </items>
    </Table>

</c:FragmentDefinition>